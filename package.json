{
  "name": "kookaburra",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "husky": "5.0.9",
    "jest": "26.6.3",
    "lerna": "3.22.1",
    "snazzy": "9.0.0",
    "standard": "16.0.3"
  },
  "scripts": {
    "test": "standard --env jest --verbose | snazzy && NODE_OPTIONS=--experimental-vm-modules jest",
    "fix": "standard --env jest --fix",
    "prepare": "husky install"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
